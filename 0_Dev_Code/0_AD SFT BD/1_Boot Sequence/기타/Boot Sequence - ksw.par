################################################   POWER GOOD CHECK    #######################################################					
############################################################################################################################					

fncBegin		#name:POWER_GOOD_CHECK#			

# 1.2~5V Power GOOD Check (PG_PM1~4, PG_5VDC)
#i4fill		0x60000200	0x00000000	4	
i4bufclr	0		# Buffer[0] Clear 				
i4bufclr	1		# Buffer[1] Clear 

# 반복문 시작 (최대 5회)
lblnamed			#name:POWER_GOOD_CHECK_1#		
wf32		0x00098000	2 1 0	# ADC.CONTROL.[2]mode : single shot mode 
rf32		0x000A830C	6 4		# GPIO.PIDR1_HIGH # PG_PM1~4 pin check 
i4drshiftl	1					# DREG0 <<= 1, PG_5VDC를 위한 bit 확보 
i4drsave	1					# Buffer[1] = DREG0, PG_PM1~4 bit 정보 

rf32		0x000A830C	2 1		# GPIO.PIDR1_HIGH # PG_5VDC pin check 
i4drcheck	0x00000001			# 	
	jmpnamed	always	0		#name:ADC_1_CHECK# GO TO 		
	i4drrestore	1				# PG_5VDC pin OK # DREG1 = DREG0, DREG0 = Buffer[1] 
i4dradd		0x00000001			# DREG0++ 
i4drsave	1					# Buffer[1] = DREG0, 모든 PG에 대한 bit 정보 확보  
lblnamed			#name:ADC_1_CHECK#	# PG_5VDC가 문제 있으므로, 11110b가 되어 있음. 		

#	PG_PM1 | PG_PM2 | PG_PM3 | PG_PM4 | PG_5VDC 
#        1 |      1 |      1 |      1 |       1 b  (0x1F) Buffer[1], IF Power Good until now 

# 12V Power GOOD Check # 
# 반복문 시작 # ADC measurement 완료 될 때까지 반복 
lblnamed			#name:TRIGGER_CHECK_1#		
rf32		0x00098000	0 2	# ADC.CONTROL.[1:0]Trigger 
i4drcheck	0x00000000		# IF Trigger == OFF 	
	jmpnamed	always	0	#name:TRIGGER_CHECK_1#	ADC measurement not completed??? 	
	# 반복문 끝 
	w32		0x00098000	0x00005505	# ADC.CONTROL# # Start & Stop CH 5, Single shot mode, Trigger immediate 

d	20				
# ADC6(12V)에 대한 error check 
rf32		0x00098024	13 1		# ADC.CHANNEL_5.[13]error_5 : Rnage error detected 
i4drcheck	0x00000000				
	jmpnamed	always	0	#name:ADC_6_CHECK#	GO TO - error detected 		
	i4drrestore	1					# DREG1 = DREG0, DREG0 = Buffer[1], Error NOT detected 
i4dradd		0x00000020				# DREG0 += 0x20 
i4drsave	1						# Buffer[1] = DREG0, 모두 정상이면 0x3F(111111b)가 됨. 
lblnamed			#name:ADC_6_CHECK#		

#	ADC6(12V) | PG_PM1 | PG_PM2 | PG_PM3 | PG_PM4 | PG_5VDC 
#           1 |      1 |      1 |      1 |      1 |       1 b  (0x3F) Buffer[1], IF Power Good until now 

# Power FAIL Check #
i4drrestore	1					# DREG1 = DREG0, DREG0 = Buffer[1] 
i4drload	0x0000003F			# DREG1 = DREG0, DREG0 = 0x3F 
jmpnamed	==	0x00000000	#name:POWER_GOOD_1# IF(DREG1==DREG0) 
	i4bufinc	0
i4bufcheck	0	5				# 반복문 5회 count 
	jmpnamed	!=	0x00000000	#name:POWER_GOOD_CHECK_1#		
	#반복문 끝 
	i4drload	0xFFFFFFFF		# DREG1 = DREG0, DREG0 = 0xFFFFFFFF	
i4drload	0x60000200			# DREG1 = DREG0, DREG0 = 0x60000200, SRAM.Power_GOOD_S 
i4poke	32						# SRAM.Power_GOOD_S = 0xF~F, PG NG 
jmpnamed	always	0	#name:POWER_BAD_1#		GO TO 
					
lblnamed			#name:POWER_GOOD_1#		
i4drload	0x11111111			# DREG1 = DREG0, DREG0 = 0x11111111 			
i4drload	0x60000200			# DREG1 = 0x11111111, DREG0 = 0x60000200, SRAM.Power_GOOD_S 
i4poke	32						# SRAM.Power_GOOD_S = 0x1~1, Power GOOD 

lblnamed			#name:POWER_BAD_1#		
i4drrestore	1					# DREG1 = DREG0, DREG0 = Buffer[1] 
i4drload	0x60000204			# DREG1 = DREG0, DREG0 = 0x60000204, SRAM.CHECK_RE_S 
i4poke	32						# SRAM.CHECK_RE_S = Buffer[1] 
# POWER_BAD_1는 Power Good, Bad 상관 없이 무조건 수행
# Power Good이면 Buffer[1] == 0x3F 
# Power Bad라면 Buffer[1] != 0x3F 

wf32	0x00098000	2 1 0	# ADC.CONTROL.[2]Mode : Single Shot mode 

fncEnd		#name:POWER_GOOD_CHECK#


